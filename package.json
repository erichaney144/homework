{
	"name": "symetra-project",
	"version": "1.0.0",
	"main": "dist/server.js",
	"license": "MIT",
	"scripts": {
		"build": "npm run clean && tsc",
		"clean": "rm -rf dist",
		"watch": "tsc -w",
		"dev:distro": "export NODE_PATH=dist; nodemon ./dist/server.js",
		"test": "jest",
		"start": "export NODE_PATH=dist; node dist/server.js"
	},
	"dependencies": {
		"express": "^4.18.2",
		"express-session": "^1.17.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.15",
		"@types/express-session": "^1.17.5",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.18",
		"jest": "^29.3.1",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.1",
		"typescript": "^4.9.4"
	},
	"jest": {
		"collectCoverage": true,
		"testMatch": [
			"**/?(*.)test.js"
		],
		"modulePaths": [
			"dist"
		]
	}
}
